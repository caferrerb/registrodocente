	<queryString>
		<![CDATA[SELECT R.fecha,R.comentario,C.ID_CURSO,A.Nombre AS A_NOM FROM (((T_REGISTRO R JOIN T_SESION_CURSO SC ON R.sesion = SC.ID_SESIONCURSO) JOIN T_CURSO C ON SC.ID_CURSO = C.ID_CURSO) JOIN T_ASIGNATURA A ON C.asignatura = A.ID_ASIGNATURA) JOIN T_DOCENTE D ON C.docente = D.ID_USUARIO WHERE R.aprobcoord = TRUE AND R.aprobrh = TRUE AND D.ID_USUARIO = $P{DOCENTE} AND A.ID_ASIGNATURA = $P{asig_id} order by C.ID_CURSO,R.fecha]]>
